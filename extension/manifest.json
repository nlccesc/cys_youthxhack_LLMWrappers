{
  "manifest_version": 3,
  "name": "Security Chrome Extension",
  "version": "1.0",
  "description": "A Chrome extension to verify the legitimacy of digital content.",
  "permissions": [
      "activeTab",
      "storage",
      "notifications"
  ],
  "background": {
      "service_worker": "background/background.js"
  },
  "action": {
      "default_popup": "popup/popup.html"
  }
}
